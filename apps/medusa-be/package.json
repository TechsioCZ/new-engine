{
  "name": "medusa-be",
  "version": "0.0.1",
  "description": "A starter for Medusa projects.",
  "author": "Medusa (https://medusajs.com)",
  "license": "MIT",
  "keywords": [
    "sqlite",
    "postgres",
    "typescript",
    "ecommerce",
    "headless",
    "medusa"
  ],
  "scripts": {
    "build": "medusa build",
    "start": "medusa start",
    "start:dev": "cross-env NODE_ENV=development medusa start",
    "dev": "medusa develop",
    "test:integration:http": "cross-env TEST_TYPE=integration:http NODE_OPTIONS=--experimental-vm-modules jest --silent=false --runInBand --forceExit",
    "test:integration:modules": "cross-env TEST_TYPE=integration:modules NODE_OPTIONS=--experimental-vm-modules jest --silent --runInBand --forceExit",
    "test:unit": "cross-env TEST_TYPE=unit NODE_OPTIONS=--experimental-vm-modules jest --silent --runInBand --forceExit",
    "test:all": "pnpm test:unit && pnpm test:integration:modules && pnpm test:integration:http",
    "migrateProd": "medusa db:migrate --execute-all-links",
    "migrate": "medusa db:migrate",
    "migrate:generate-only": "medusa db:generate",
    "syncLinks": "medusa db:sync-links",
    "createInitialSuperadmin": "medusa user -e \"$SUPERADMIN_EMAIL\" -p \"$SUPERADMIN_PASSWORD\"",
    "seedDevData": "medusa exec ./src/scripts/seed-dev-data.ts",
    "seedN1": "medusa exec ./src/scripts/seed-n1.ts",
    "seedInitialData": "medusa exec ./src/scripts/seed.ts",
    "seedInitialDataTs": "medusa exec ./src/scripts/seed.ts",
    "seedDefaultData": "medusa exec ./src/scripts/seed-products-from-db.ts",
    "seedDefaultDataTs": "medusa exec ./src/scripts/seed-products-from-db.ts",
    "addInitialSearchDocuments": "medusa exec ./src/scripts/search-index.ts",
    "setInitialPublishableKey": "bash -c \"medusa exec ./src/scripts/output-publishable-key.ts | grep -o '<PK_TOKEN>.*</PK_TOKEN>' | sed 's/<PK_TOKEN>//g;s/<\\/PK_TOKEN>//g' | tr -d '\\n\\r' | zsc setSecretEnv CHANNEL_PUBLISHABLE_KEY\""
  },
  "dependencies": {
    "@badgateway/oauth2-client": "^3.3.1",
    "@medusajs/admin-sdk": "^2.13.1",
    "@medusajs/admin-shared": "^2.13.1",
    "@medusajs/caching-redis": "^2.13.1",
    "@medusajs/cli": "^2.13.1",
    "@medusajs/dashboard": "^2.13.1",
    "@medusajs/draft-order": "^2.13.1",
    "@medusajs/event-bus-redis": "^2.13.1",
    "@medusajs/framework": "^2.13.1",
    "@medusajs/icons": "^2.13.1",
    "@medusajs/index": "^2.13.1",
    "@medusajs/js-sdk": "^2.13.1",
    "@medusajs/medusa": "^2.13.1",
    "@medusajs/ui": "^4.1.1",
    "@neondatabase/serverless": "^1.0.2",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/context-async-hooks": "^2.4.0",
    "@opentelemetry/core": "^2.4.0",
    "@opentelemetry/exporter-trace-otlp-grpc": "^0.210.0",
    "@opentelemetry/sdk-trace-base": "^2.4.0",
    "@opentelemetry/semantic-conventions": "^1.39.0",
    "@rokmohar/medusa-plugin-meilisearch": "^1.3.5",
    "@sentry/node": "^10.29.0",
    "@sentry/opentelemetry": "^10.34.0",
    "@tanstack/react-query": "5.64.2",
    "drizzle-orm": "^0.45.0",
    "mime": "4.1.0",
    "mysql2": "^3.15.3",
    "strong-soap": "^1.20.0",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@medusajs/test-utils": "^2.13.1",
    "@swc/core": "^1.15.3",
    "@swc/jest": "^0.2.39",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.3",
    "@types/react-dom": "^19.2.3",
    "cross-env": "^10.1.0",
    "jest": "^30.2.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "typescript": "^5.9.3",
    "vite": "^5.4.21",
    "yalc": "1.0.0-pre.53",
    "@medusajs/types": "^2.12.5",
    "@hookform/resolvers": "^3.4.2",
    "@radix-ui/react-label": "^2.1.1",
    "@radix-ui/react-popover": "^1.1.5",
    "@radix-ui/react-radio-group": "^1.2.2",
    "@radix-ui/react-slot": "^1.1.1",
    "@tanstack/react-table": "^8.20.5",
    "@uiw/react-json-view": "^2.0.0-alpha.40",
    "cmdk": "^0.2.1",
    "date-fns": "^3.6.0",
    "i18next": "^23.7.11",
    "lodash": "^4.17.21",
    "react-hook-form": "^7.49.1",
    "react-i18next": "^13.5.0",
    "react-router-dom": "^6.30.1"
  },
  "engines": {
    "node": ">=22"
  }
}
