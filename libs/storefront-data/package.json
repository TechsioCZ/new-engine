{
  "name": "@techsio/storefront-data",
  "version": "0.1.0",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/src/client/provider.d.ts",
      "import": "./dist/client/provider.js"
    },
    "./server/get-query-client": {
      "types": "./dist/src/server/get-query-client.d.ts",
      "import": "./dist/server/get-query-client.js"
    },
    "./get-query-client": null,
    "./auth/*": {
      "types": "./dist/src/auth/*.d.ts",
      "import": "./dist/auth/*.js"
    },
    "./cart/*": {
      "types": "./dist/src/cart/*.d.ts",
      "import": "./dist/cart/*.js"
    },
    "./catalog/*": {
      "types": "./dist/src/catalog/*.d.ts",
      "import": "./dist/catalog/*.js"
    },
    "./categories/*": {
      "types": "./dist/src/categories/*.d.ts",
      "import": "./dist/categories/*.js"
    },
    "./checkout/*": {
      "types": "./dist/src/checkout/*.d.ts",
      "import": "./dist/checkout/*.js"
    },
    "./client/*": {
      "types": "./dist/src/client/*.d.ts",
      "import": "./dist/client/*.js"
    },
    "./collections/*": {
      "types": "./dist/src/collections/*.d.ts",
      "import": "./dist/collections/*.js"
    },
    "./customers/*": {
      "types": "./dist/src/customers/*.d.ts",
      "import": "./dist/customers/*.js"
    },
    "./orders/*": {
      "types": "./dist/src/orders/*.d.ts",
      "import": "./dist/orders/*.js"
    },
    "./products/*": {
      "types": "./dist/src/products/*.d.ts",
      "import": "./dist/products/*.js"
    },
    "./regions/*": {
      "types": "./dist/src/regions/*.d.ts",
      "import": "./dist/regions/*.js"
    },
    "./shared/*": {
      "types": "./dist/src/shared/*.d.ts",
      "import": "./dist/shared/*.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rslib build",
    "dev": "rslib build --watch",
    "lint": "pnpm -w exec biome lint libs/storefront-data/src --diagnostic-level=error",
    "test": "vitest run --config vitest.config.ts"
  },
  "nx": {
    "targets": {
      "test": {
        "executor": "nx:run-commands",
        "options": {
          "command": "pnpm test",
          "cwd": "libs/storefront-data"
        },
        "cache": true
      }
    }
  },
  "devDependencies": {
    "@rsbuild/plugin-react": "^1.4.1",
    "@rslib/core": "^0.18.0",
    "@testing-library/react": "^16.3.1",
    "@types/react": "^19.2.3",
    "@types/react-dom": "^19.2.3",
    "jsdom": "^27.4.0",
    "msw": "^2.8.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.17"
  },
  "peerDependencies": {
    "@medusajs/js-sdk": ">=2.12.0",
    "@medusajs/types": ">=2.12.0",
    "@tanstack/react-query": ">=5.0.0",
    "react": ">=19.2.0",
    "react-dom": ">=19.2.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
